\r\n                <vs-th>Name</vs-th>\r\n                <vs-th>Website</vs-th>\r\n                <vs-th>Nro</vs-th>\r\n            </template>\r\n\r\n            <template slot-scope=\"{data}\">\r\n                <vs-tr :data=\"tr\" :key=\"indextr\" v-for=\"(tr, indextr) in data\">\r\n\r\n                    <vs-td :data=\"data[indextr].email\">\r\n                        {{data[indextr].email}}\r\n                    </vs-td>\r\n\r\n                    <vs-td :data=\"data[indextr].username\">\r\n                        {{data[indextr].username}}\r\n                    </vs-td>\r\n\r\n                    <vs-td :data=\"data[indextr].website\">\r\n                        {{data[indextr].website}}\r\n                    </vs-td>\r\n\r\n                    <vs-td :data=\"data[indextr].id\">\r\n                        {{data[indextr].id}}\r\n                    </vs-td>\r\n\r\n                </vs-tr>\r\n            </template>\r\n        </vs-table>\r\n\r\n        <br>\r\n\r\n        <prism language=\"js\" class=\"rounded-lg\">\r\n{{ selected }}\r\n        </prism>\r\n\r\n        <template slot=\"codeContainer\">\r\n&lt;template&gt;\r\n    &lt;vs-table multiple v-model=&quot;selected&quot; @selected=&quot;handleSelected&quot; :data=&quot;users&quot;&gt;\r\n\r\n      &lt;template slot=&quot;thead&quot;&gt;\r\n        &lt;vs-th&gt;Email&lt;/vs-th&gt;\r\n        &lt;vs-th&gt;Name&lt;/vs-th&gt;\r\n        &lt;vs-th&gt;Website&lt;/vs-th&gt;\r\n        &lt;vs-th&gt;Nro&lt;/vs-th&gt;\r\n      &lt;/template&gt;\r\n\r\n      &lt;template slot-scope=&quot;{data}&quot;&gt;\r\n        &lt;vs-tr :data=&quot;tr&quot; :key=&quot;indextr&quot; v-for=&quot;(tr, indextr) in data&quot;&gt;\r\n\r\n          &lt;vs-td :data=&quot;data[indextr].email&quot;&gt;\r\n            {{ \"\\{\\{ data[indextr].email \\}\\}\" }}\r\n          &lt;/vs-td&gt;\r\n\r\n          &lt;vs-td :data=&quot;data[indextr].username&quot;&gt;\r\n            {{ \"\\{\\{ data[indextr].username \\}\\}\" }}\r\n          &lt;/vs-td&gt;\r\n\r\n          &lt;vs-td :data=&quot;data[indextr].website&quot;&gt;\r\n            {{ \"\\{\\{ data[indextr].website \\}\\}\" }}\r\n          &lt;/vs-td&gt;\r\n\r\n          &lt;vs-td :data=&quot;data[indextr].id&quot;&gt;\r\n            {{ \"\\{\\{ data[indextr].id \\}\\}\" }}\r\n          &lt;/vs-td&gt;\r\n\r\n        &lt;/vs-tr&gt;\r\n      &lt;/template&gt;\r\n    &lt;/vs-table&gt;\r\n\r\n    &lt;br&gt;\r\n\r\n    &lt;pre&gt;{{ \"\\{\\{ selected \\}\\}\" }}&lt;/pre&gt;\r\n&lt;/template&gt;\r\n\r\n&lt;script&gt;\r\nexport default {\r\n  data() {\r\n    return {\r\n      selected: [],\r\n      users: [.....],\r\n    }\r\n  },\r\n  methods: {\r\n    handleSelected(tr) {\r\n      this.$vs.notify({\r\n        title: `Selected ${tr.username}`,\r\n        text: `Email: ${tr.email}`\r\n      })\r\n    }\r\n  }\r\n}\r\n&lt;/script&gt;\r\n        </template>\r\n\r\n    </vx-card>\r\n</template>\r\n\r\n<script>\r\nimport Prism from 'vue-prism-component'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      selected: [],\r\n      users: [\r\n        {\r\n          'id': 1,\r\n          'name': 'Leanne Graham',\r\n          'username': 'Bret',\r\n          'email': 'Sincere@april.biz',\r\n          'website': 'hildegard.org'\r\n        },\r\n        {\r\n          'id': 2,\r\n          'name': 'Ervin Howell',\r\n          'username': 'Antonette',\r\n          'email': 'Shanna@melissa.tv',\r\n          'website': 'anastasia.net'\r\n        },\r\n        {\r\n          'id': 3,\r\n          'name': 'Clementine Bauch',\r\n          'username': 'Samantha',\r\n          'email': 'Nathan@yesenia.net',\r\n          'website': 'ramiro.info'\r\n        },\r\n        {\r\n          'id': 4,\r\n          'name': 'Patricia Lebsack',\r\n          'username': 'Karianne',\r\n          'email': 'Julianne.OConner@kory.org',\r\n          'website': 'kale.biz'\r\n        },\r\n        {\r\n          'id': 5,\r\n          'name': 'Chelsey Dietrich',\r\n         