:src=\"dataImg\" alt=\"img\" class=\"responsive\">\r\n        </div>\r\n\r\n        <!-- Image upload Buttons -->\r\n        <div class=\"modify-img flex justify-between mt-5\">\r\n          <input type=\"file\" class=\"hidden\" ref=\"updateImgInput\" @change=\"updateCurrImg\" accept=\"image/*\">\r\n          <vs-button class=\"mr-4\" type=\"flat\" @click=\"$refs.updateImgInput.click()\">上传图片</vs-button>\r\n          <vs-button type=\"flat\" color=\"#999\" @click=\"dataImg = null\">删除图片</vs-button>\r\n        </div>\r\n      </template>\r\n      <div class=\"upload-img mt-5\" v-if=\"!dataImg\">\r\n        <input type=\"file\" class=\"hidden\" ref=\"uploadImgInput\" @change=\"updateCurrImg\" accept=\"image/*\">\r\n        <vs-button @click=\"$refs.uploadImgInput.click()\">上传图片</vs-button>\r\n      </div>\r\n\r\n      <div class=\"p-6\">\r\n        <!-- NAME -->\r\n        <vs-input\r\n          label=\"分类名称\"\r\n          v-model=\"name\"\r\n          class=\"mt-5 w-full\"\r\n          name=\"item-name\"\r\n          \r\n        />\r\n        <span class=\"text-danger text-sm\" v-show=\"errors.has('item-name')\">{{\r\n          errors.first(\"item-name\")\r\n        }}</span>\r\n\r\n        <vs-input\r\n          label=\"背景色\"\r\n          v-model=\"color\"\r\n          class=\"mt-5 w-full\"\r\n          name=\"item-name\"\r\n          \r\n        />\r\n        <span class=\"text-danger text-sm\" v-show=\"errors.has('item-name')\">{{\r\n          errors.first(\"item-name\")\r\n        }}</span>\r\n\r\n\r\n        <vs-input\r\n          label=\"排序\"\r\n          v-model=\"order\"\r\n          class=\"mt-5 w-full\"\r\n          name=\"item-order\"\r\n          \r\n        />\r\n        <span class=\"text-danger text-sm\" v-show=\"errors.has('item-order')\">{{\r\n          errors.first(\"item-order\")\r\n        }}</span>\r\n\r\n      </div>\r\n    </component>\r\n\r\n    <div class=\"flex flex-wrap items-center p-6\" slot=\"footer\">\r\n      <!-- <vs-button class=\"mr-6\" @click=\"submitData\" :disabled=\"!isFormValid\">提交</vs-button> -->\r\n      <vs-button class=\"mr-6\" @click=\"submitData\">提交</vs-button>\r\n      <vs-button\r\n        type=\"border\"\r\n        color=\"danger\"\r\n        @click=\"isSidebarActiveLocal = false\"\r\n        >取消</vs-button\r\n      >\r\n    </div>\r\n  </vs-sidebar>\r\n</template>\r\n\r\n<script>\r\nimport VuePerfectScrollbar from \"vue-perfect-scrollbar\";\r\n\r\nexport default {\r\n  props: {\r\n    isSidebarActive: {\r\n      type: Boolean,\r\n      required: true,\r\n    },\r\n    data: {\r\n      type: Object,\r\n      default: () => {},\r\n    },\r\n  },\r\n  components: {\r\n    VuePerfectScrollbar,\r\n  },\r\n  data() {\r\n    return {\r\n      boxId: null,\r\n      name: \"\",\r\n      order: \"\",\r\n      color:\"\",\r\n      settings: {\r\n        // perfectscrollbar settings\r\n        maxScrollbarLength: 60,\r\n        wheelSpeed: 0.6,\r\n      },\r\n      file:'',\r\n      dataImg:''\r\n    };\r\n  },\r\n  watch: {\r\n    isSidebarActive(val) {\r\n      if (!val) return;\r\n      console.log(this.data);\r\n      if(Object.entries(this.data).length === 0){\r\n        this.initValues();\r\n      }else{\r\n        const { id, name, order,color,img} = JSON.parse(JSON.stringify(this.data));\r\n        this.dataId = id;\r\n        this.name = name;\r\n        this.order = order;\r\n        this.color = color;\r\n        this.dataImg = img;\r\n      }\r\n    },\r\n  },\r\n  computed: {\r\n    isSidebarActiveLocal: {\r\n      get() {\r\n        return this.isSidebarActive;\r\n      },\r\n      set(val) {\r\n        if (!val) {\r\n          this.$emit(\"closeSidebar\");\r\n          // this.$validator.reset()\r\n          // this.initValues()\r\n        }\r\n      },\r\n    },\r\n    isFormValid() {\r\n      return (\r\n        !this.errors.any() &&\r\n        this.name\r\n      );\r\n    },\r\n    scrollbarTag() {\r\n      return this.$store.getters.scrollbarTag;\r\n    },\r\n  },\r\n  methods: {\r\n    initValues() {\r\n      if (this.data.id) return;\r\n      this.dataId = null;\r\n      this.name = \"\";\r\n      this.order = \"\";\r\n      this.dataImg = \"\";\r\n    },\r\n    submitData() {\r\n      let _this = this;\r\n      const typeInfo = {\r\n        id:_this.dataId,\r\n        name: _this.name,\r\n        order:_this.order,\r\n        color:_this.color\r\n      };\r\n      var forms = new FormData()\r\n      forms.append('data',JSON.stringify(typeInfo));\r\n      forms.append('img',_this.imgFile);\r\n      let url = \"/admin/All_skin/addSkinType\";\r\n      if(this.dataId>0){\r\n        url = \"/admin/All_skin/editSkinType\";\r\n      }\r\n      _this.$axios({\r\n          url:url,\r\n          method: \"post\",\r\n          data:forms,\r\n          headers:{'Content-Type':'multipart/form-data'}\r\n        }).then((res) => {\r\n          if(res.data.status == 1){\r\n             _this.$vs.notify({\r\n              title: '提示',\r\n              text: res.data.msg,\r\n              iconPack: 'feather',\r\n              icon: 'icon-alert-circle',\r\n              color: 'success'\r\n            })\r\n            this.$emit(\"closeSidebar\");\r\n            this.$emit(\"reload\");\r\n            this.initValues();\r\n          }else{\r\n            _this.$vs.notify({\r\n              title: '提示',\r\n              text: res.data.msg,\r\n              iconPack: 'feather',\r\n              icon: 'icon-alert-circle',\r\n              color: 'danger'\r\n            })\r\n          }\r\n        });\r\n    },\r\n    updateCurrImg (input) {\r\n      if (input.target.files && input.target.files[0]) {\r\n        const reader = new FileReader()\r\n        reader.onload = e => {\r\n          this.dataImg = e.target.result;\r\n          // this.robot.img = e.target.result;\r\n          this.imgFile = input.target.files[0];\r\n        }\r\n        reader.readAsDataURL(input.target.files[0])\r\n      }\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.add-new-data-sidebar {\r\n  ::v-deep .vs-sidebar--background {\r\n    z-index: 52010;\r\n  }\r\n\r\n  ::v-deep .vs-sidebar {\r\n    z-index: 52010;\r\n    width: 400px;\r\n    max-width: 90vw;\r\n\r\n    .img-upload {\r\n      margin-top: 2rem;\r\n\r\n      .con-img-upload {\r\n        padding: 0;\r\n      }\r\n\r\n      .con-input-upload {\r\n        width: 100%;\r\n        margin: 0;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.scroll-area--data-list-add-new {\r\n  // height: calc(var(--vh, 1vh) * 100 - 4.3rem);\r\n  height: calc(var(--vh, 1vh) * 100 - 16px - 45px - 82px);\r\n\r\n  &:not(.ps) {\r\n    overflow-y: auto;\r\n  }\r\n}\r\n.upload-img{\r\n  padding-left: 20px;\r\n}\r\n</style>\r\n"]}]}