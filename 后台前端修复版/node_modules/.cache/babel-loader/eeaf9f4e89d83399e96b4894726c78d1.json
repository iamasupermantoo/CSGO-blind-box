o-padding\"\r\n    spacer\r\n    v-model=\"isSidebarActiveLocal\"\r\n  >\r\n    <div class=\"mt-6 flex items-center justify-between px-6\">\r\n      <h4>\r\n        {{ Object.entries(this.data).length === 0 ? \"添加\" : \"编辑\" }} 充值汇率\r\n      </h4>\r\n      <feather-icon\r\n        icon=\"XIcon\"\r\n        @click.stop=\"isSidebarActiveLocal = false\"\r\n        class=\"cursor-pointer\"\r\n      ></feather-icon>\r\n    </div>\r\n    <!-- <vs-divider class=\"mb-0\"></vs-divider> -->\r\n\r\n    <component\r\n      :is=\"scrollbarTag\"\r\n      class=\"scroll-area--data-list-add-new\"\r\n      :settings=\"settings\"\r\n      :key=\"$vs.rtl\"\r\n    >\r\n\r\n      <div class=\"p-6\">\r\n        <!-- NAME -->\r\n        <vs-input\r\n          label=\"设置汇率\"\r\n          v-model=\"exchange_rate\"\r\n          class=\"mt-5 w-full\"\r\n          name=\"item-type_name\"\r\n          \r\n        />\r\n        <span class=\"text-danger text-sm\" v-show=\"errors.has('item-type_name')\">{{\r\n          errors.first(\"item-type_name\")\r\n        }}</span>\r\n\r\n        <vs-input\r\n          label=\"饰品价格比例\"\r\n          v-model=\"bili\"\r\n          class=\"mt-5 w-full\"\r\n          name=\"item-type_name\"\r\n          \r\n        />\r\n        <span class=\"text-danger text-sm\" v-show=\"errors.has('item-type_name')\">{{\r\n          errors.first(\"item-type_name\")\r\n        }}</span>\r\n\r\n      </div>\r\n    </component>\r\n\r\n    <div class=\"flex flex-wrap items-center p-6\" slot=\"footer\">\r\n      <!-- <vs-button class=\"mr-6\" @click=\"submitData\" :disabled=\"!isFormValid\">提交</vs-button> -->\r\n      <vs-button class=\"mr-6\" @click=\"submitData\">提交</vs-button>\r\n      <vs-button\r\n        type=\"border\"\r\n        color=\"danger\"\r\n        @click=\"isSidebarActiveLocal = false\"\r\n        >取消</vs-button\r\n      >\r\n    </div>\r\n  </vs-sidebar>\r\n</template>\r\n\r\n<script>\r\nimport VuePerfectScrollbar from \"vue-perfect-scrollbar\";\r\n\r\nexport default {\r\n  props: {\r\n    isSidebarActive: {\r\n      type: Boolean,\r\n      required: true,\r\n    },\r\n    data: {\r\n      type: Object,\r\n      default: () => {},\r\n    },\r\n  },\r\n  components: {\r\n    VuePerfectScrollbar,\r\n  },\r\n  data() {\r\n    return {\r\n      boxId: null,\r\n      exchange_rate: \"\",\r\n      bili:'',\r\n      settings: {\r\n        // perfectscrollbar settings\r\n        maxScrollbarLength: 60,\r\n        wheelSpeed: 0.6,\r\n      },\r\n      file:''\r\n    };\r\n  },\r\n  watch: {\r\n    isSidebarActive(val) {\r\n      if (!val) return;\r\n      console.log(this.data);\r\n      if(Object.entries(this.data).length === 0){\r\n        this.initValues();\r\n      }else{\r\n        const { id, exchange_rate,bili} = JSON.parse(JSON.stringify(this.data));\r\n        this.dataId = id;\r\n        this.bili   = bili;\r\n        this.exchange_rate = exchange_rate;\r\n      }\r\n    },\r\n  },\r\n  computed: {\r\n    isSidebarActiveLocal: {\r\n      get() {\r\n        return this.isSidebarActive;\r\n      },\r\n      set(val) {\r\n        if (!val) {\r\n          this.$emit(\"closeSidebar\");\r\n          // this.$validator.reset()\r\n          // this.initValues()\r\n        }\r\n      },\r\n    },\r\n    isFormValid() {\r\n      return (\r\n        !this.errors.any() &&\r\n        this.type_name\r\n      );\r\n    },\r\n    scrollbarTag() {\r\n      return this.$store.getters.scrollbarTag;\r\n    },\r\n  },\r\n  methods: {\r\n    initValues() {\r\n      if (this.data.id) return;\r\n        this.dataId = '';\r\n        this.bili   = '';\r\n        this.exchange_rate = '';\r\n    },\r\n    submitData() {\r\n      let _this = this;\r\n      const info = {\r\n        exchange_rate:_this.exchange_rate,\r\n        bili:_this.bili\r\n      };\r\n        _this.$axios({\r\n            url: \"/admin/Setting/exchangeRate\",\r\n            method: \"post\",\r\n            data:info,\r\n        }).then((res) => {\r\n            if(res.data.status == 1){\r\n                _this.$vs.notify({\r\n                title: '提示',\r\n                text: res.data.msg,\r\n                iconPack: 'feather',\r\n                icon: 'icon-alert-circle',\r\n                color: 'success'\r\n                })\r\n                this.$emit(\"closeSidebar\");\r\n                this.$emit(\"reload\");\r\n                this.initValues();\r\n            }else{\r\n                _this.$vs.notify({\r\n                title: '提示',\r\n                text: res.data.msg,\r\n                iconPack: 'feather',\r\n                icon: 'icon-alert-circle',\r\n                color: 'danger'\r\n                })\r\n            }\r\n        });\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.add-new-data-sidebar {\r\n  ::v-deep .vs-sidebar--background {\r\n    z-index: 52010;\r\n  }\r\n\r\n  ::v-deep .vs-sidebar {\r\n    z-index: 52010;\r\n    width: 400px;\r\n    max-width: 90vw;\r\n\r\n    .img-upload {\r\n      margin-top: 2rem;\r\n\r\n      .con-img-upload {\r\n        padding: 0;\r\n      }\r\n\r\n      .con-input-upload {\r\n        width: 100%;\r\n        margin: 0;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.scroll-area--data-list-add-new {\r\n  // height: calc(var(--vh, 1vh) * 100 - 4.3rem);\r\n  height: calc(var(--vh, 1vh) * 100 - 16px - 45px - 82px);\r\n\r\n  &:not(.ps) {\r\n    overflow-y: auto;\r\n  }\r\n}\r\n.upload-img{\r\n    margin-left: 20px;\r\n}\r\n.img-container{\r\n    width: 100px!important;\r\n}\r\n</style>\r\n"]}]}