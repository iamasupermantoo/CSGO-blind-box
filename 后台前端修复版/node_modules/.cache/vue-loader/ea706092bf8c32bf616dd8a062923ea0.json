lbarTag\"\r\n      class=\"scroll-area--data-list-add-new\"\r\n      :settings=\"settings\"\r\n      :key=\"$vs.rtl\"\r\n    >\r\n\r\n      <div class=\"p-6\">\r\n        <vs-input label=\"请输入需要增加的余额(正数为增加,负数减少)\" v-model=\"balance\" class=\"mt-5 w-full\" name=\"item-type_name\"/>\r\n        <span class=\"text-danger text-sm\" v-show=\"errors.has('item-type_name')\">{{errors.first(\"item-type_name\")}}</span>\r\n      </div>\r\n\r\n    </component>\r\n\r\n    <div class=\"flex flex-wrap items-center p-6\" slot=\"footer\">\r\n      <vs-button class=\"mr-6\" @click=\"submitData\">提交</vs-button>\r\n      <vs-button\r\n        type=\"border\"\r\n        color=\"danger\"\r\n        @click=\"isSidebarActiveLocal = false\"\r\n        >取消</vs-button\r\n      >\r\n    </div>\r\n  </vs-sidebar>\r\n</template>\r\n\r\n<script>\r\nimport VuePerfectScrollbar from \"vue-perfect-scrollbar\";\r\n\r\nexport default {\r\n  props: {\r\n    isSidebarActive: {\r\n      type: Boolean,\r\n      required: true,\r\n    },\r\n  },\r\n  components: {\r\n    VuePerfectScrollbar,\r\n  },\r\n  data() {\r\n    return {\r\n      boxId: null,\r\n      accessKeyId: \"\",\r\n      accessKeySecret:\"\",\r\n      endpoint:\"\",\r\n      bucket:\"\",\r\n      balance:'',\r\n      settings: {\r\n        // perfectscrollbar settings\r\n        maxScrollbarLength: 60,\r\n        wheelSpeed: 0.6,\r\n      },\r\n      file:''\r\n    };\r\n  },\r\n  watch: {\r\n    isSidebarActive(val) {\r\n      if (!val) return;\r\n    },\r\n  },\r\n  computed: {\r\n    isSidebarActiveLocal: {\r\n      get() {\r\n        return this.isSidebarActive;\r\n      },\r\n      set(val) {\r\n        if (!val) {\r\n          this.$emit(\"closeSidebar\");\r\n        }\r\n      },\r\n    },\r\n    isFormValid() {\r\n      return (\r\n        !this.errors.any() &&\r\n        this.type_name\r\n      );\r\n    },\r\n    scrollbarTag() {\r\n      return this.$store.getters.scrollbarTag;\r\n    },\r\n  },\r\n  methods: {\r\n    initValues() {\r\n      this.balance = ''\r\n    },\r\n    submitData() {\r\n      let _this = this;\r\n      if( _this.balance == undefined || _this.balance == '' ) {\r\n        _this.$vs.notify({\r\n          title: '提示',\r\n          text: \"请输入余额\",\r\n          iconPack: 'feather',\r\n          icon: 'icon-alert-circle',\r\n          color: 'danger'\r\n        })\r\n        return;\r\n      }\r\n\r\n      const info = {\r\n        balance:_this.balance\r\n      };\r\n\r\n      console.log(info);\r\n        _this.$axios({\r\n            url: \"/admin/Robot/editBalance\",\r\n            method: \"post\",\r\n            data:info,\r\n        }).then((res) => {\r\n            if(res.data.status == 1){\r\n                _this.$vs.notify({\r\n                title: '提示',\r\n                text: res.data.msg,\r\n                iconPack: 'feather',\r\n                icon: 'icon-alert-circle',\r\n                color: 'success'\r\n                })\r\n                this.$emit(\"closeSidebar\");\r\n                this.$emit(\"reload\");\r\n                this.initValues();\r\n            }else{\r\n                _this.$vs.notify({\r\n                title: '提示',\r\n                text: res.data.msg,\r\n                iconPack: 'feather',\r\n                icon: 'icon-alert-circle',\r\n                color: 'danger'\r\n                })\r\n            }\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.add-new-data-sidebar {\r\n  ::v-deep .vs-sidebar--background {\r\n    z-index: 52010;\r\n  }\r\n\r\n  ::v-deep .vs-sidebar {\r\n    z-index: 52010;\r\n    width: 400px;\r\n    max-width: 90vw;\r\n\r\n    .img-upload {\r\n      margin-top: 2rem;\r\n\r\n      .con-img-upload {\r\n        padding: 0;\r\n      }\r\n\r\n      .con-input-upload {\r\n        width: 100%;\r\n        margin: 0;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.scroll-area--data-list-add-new {\r\n  // height: calc(var(--vh, 1vh) * 100 - 4.3rem);\r\n  height: calc(var(--vh, 1vh) * 100 - 16px - 45px - 82px);\r\n\r\n  &:not(.ps) {\r\n    overflow-y: auto;\r\n  }\r\n}\r\n.upload-img{\r\n    margin-left: 20px;\r\n}\r\n.img-container{\r\n    width: 100px!important;\r\n}\r\n</style>\r\n"]}]}